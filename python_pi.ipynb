   "source": [
    "import time\n",
    "\n",
    "n = int(10e9)\n",
    "\n",
    "start_time = time.time()\n",
    "\n",
    "w = 1.0/n\n",
    "psum = 0.0\n",
    "\n",
    "for i in range(1,n+1):\n",
    "    x = w*(i - 0.5)\n",
    "    psum = psum+4.0/(1.0 + x*x)\n",
    "pi=w*psum\n",
    "duration = time.time() - start_time\n",
    "\n",
    "\n",
    "print(n, \"   \", pi, \"   \", duration)"
   ]
  }
